void setup()   {                
  Serial.begin(9600);

  // by default, we'll generate the high voltage from the 3.3v line internally! (neat!)
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);  // initialize with the I2C addr 0x3D (for the 128x64)
  // init done
  
  // Show image buffer on the display hardware.
  // Since the buffer is intialized with an Adafruit splashscreen
  // internally, this will display the splashscreen.
  display.display();
  delay(2000);

  // Clear the buffer.
  display.clearDisplay();
  
loop();

}


void loop() {
  
  while (1) {
    
    int m = random(8);
    int e = random(6);
    display.clearDisplay();
    
    if(e==0) display.drawBitmap(0, 0, eoo,128,32, 1);
    if(e==1) display.drawBitmap(0, 0, evu,128,32, 1);
    if(e==2) display.drawBitmap(0, 0, evd,128,32, 1);
    if(e==3) display.drawBitmap(0, 0, eup,128,32, 1);
    if(e==4) display.drawBitmap(0, 0, eud,128,32, 1);
    if(e==5) display.drawBitmap(0, 0, esl,128,32, 1);
    if(e==6) display.drawBitmap(0, 0, eru,128,32, 1);
    
    if(m==0) display.drawBitmap(0, 32, mvp,128,32, 1);
    if(m==1) display.drawBitmap(0, 32, mvd,128,32, 1);
    if(m==2) display.drawBitmap(0, 32, mup,128,32, 1);
    if(m==3) display.drawBitmap(0, 32, mud,128,32, 1);
    if(m==4) display.drawBitmap(0, 32, mto,128,32, 1);
    if(m==5) display.drawBitmap(0, 32, msl,128,32, 1);
    if(m==6) display.drawBitmap(0, 32, moo,128,32, 1);
    if(m==7) display.drawBitmap(0, 32, mlr,128,32, 1);
    if(m==8) display.drawBitmap(0, 32, mll,128,32, 1);
    
    
    display.display();
    delay(5000);

   }
  
}


